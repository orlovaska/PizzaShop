<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:ShopWPFUI.ViewModels"
                    xmlns:view="clr-namespace:ShopWPFUI.Views">
    

    <DataTemplate DataType="{x:Type vm:ProfileViewModel}">
        <view:ProfileView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CartViewModel}">
        <view:CartView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:EmptyCartViewModel}">
        <view:EmptyCartView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PlaceOrderViewModel}">
        <view:PlaceOrderView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AuthorizationViewModel}">
        <view:AuthorizationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:RegistrationViewModel}">
        <view:RegistrationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:CatalogViewModel}">
        <view:СatalogView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type adminvm:CustomersViewModel}">
        <adminview:CustomersView/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:NavigationViewModel}">
        <view:NavigationView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProductsViewModel}">
        <view:ProductsView/>
    </DataTemplate>


</ResourceDictionary>